<Query>
<![CDATA[with temp ( dst_unit_id, src_unit_id)
as
(
select dst_unit_id,src_unit_id
from T_Unit_Relation
where dst_unit_id = '${UnitId}'
union all
select a.dst_unit_id,a.src_unit_id
from T_Unit_Relation a
inner join temp on a.src_unit_id = temp.dst_unit_id
)
select 
    SalesYear
   ,cast(SalesMonth as varchar) + '月' as SalesMonth
   ,StoreType
   ,SUM(OrderCount) as OrderCount
   ,SUM(PersonCount) as PersonCount
   ,SUM(Integral) as Integral
   ,SUM(IntegralAmount) as IntegralAmount
   ,UnitName
from
(
select 
      YEAR(cast(Date_Code as datetime)) as SalesYear 
     ,MONTH(cast(Date_Code as datetime)) as SalesMonth
	,'直营店' as StoreType
	,Donate_Integral_Order_Count as OrderCount
	,Donate_Integral_Person_Count as PersonCount
	,Donate_Integral as Integral
	,Donate_Integral_Amount as IntegralAmount
	,Unit_Name as UnitName
	,Unit_ID as UnitID
	,Customer_Id as CustomerId
from Agg_DailyUnitInfo
where Donate_Integral > 0
) as TT
where 1=1
    and StoreType='${StoreType}'
    and SalesMonth='${SalesMonth}'
    and SalesYear='${SalesYear}'
    and UnitID in (select dst_unit_id from temp)
group by 
    SalesYear   
   ,SalesMonth
   ,StoreType
   ,UnitName]]></Query>
</TableData>
