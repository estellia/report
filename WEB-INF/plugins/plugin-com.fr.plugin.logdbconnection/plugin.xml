<?xml version="1.0" encoding="UTF-8" standalone="no"?><plugin>
    <id>com.fr.plugin.logdbconnection</id>
    <name><![CDATA[自定义logdb的数据连接]]></name>
    <active>yes</active>
    <hidden>yes</hidden>
    <version>1.0</version>
    <env-version>8.0</env-version>
    <vendor>finereport</vendor>
    <description><![CDATA[通过配置plugin.xml来自由定制logdb的数据连接]]></description>
    <change-notes><![CDATA[无]]></change-notes>
    <extra-core>
         <LogDBConnection>
         <JDBCDatabaseAttr url="jdbc:sqlite://${ENV_HOME}/../FRDemo.db" driver="org.sqlite.JDBC" user="" password=""/>
         </LogDBConnection>
    </extra-core>
    <extra-report/>
    <extra-designer/>
</plugin>